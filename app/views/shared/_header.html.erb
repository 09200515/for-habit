<header class='header'>
  <div class='nav'>
    <ul class='lists-right'>
      <% if user_signed_in? %>
        <li><%= link_to current_user.nickname, "#", class: "user-nickname" %></li>
        <li><%= link_to 'ログアウト', "#", class: "logout" %></li>
      <% else %>
        <li><%= link_to 'ログイン', new_user_session_path, class: 'login' %></li>
        <li><%= link_to '新規登録', new_user_registration_path, class: 'sign-up' %>
      <% end %>
    </ul>
  </div>
</header>